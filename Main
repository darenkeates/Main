// Daren's stair project
//to drive up to 10W LED strings

int val = 0;         // variable to store the read value

void setup()
{
  void loop()
  {
  for (int ledPin=2; ledPin = 13; ledPin=ledPin+1)
    {pinMode(ledPin, OUTPUT);   // sets Pins 2-13 as output}
  for (int strainPin=0;strainPin=13;strainPin=strainPin+1)
    {pinMode(ledPin, OUTPUT);   // sets Pins 2-13 as output}
  }
}
void loop()
  {
  for (int readLoop=0;readLoop=13;readLoop=readLoop+1)
    {
    val = analogRead(readLoop);   // read the Strain guage pin
    analogWrite(readLoop, val / 4);  // analogRead values go from 0 to 1023, analogWrite values from 0 to 255
    }
   }
